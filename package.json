{
  "name": "json-formatter",
  "version": "2.0.6",
  "description": "JSON Formatter",
  "author": "Gerald <i@gerald.top>",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  },
  "scripts": {
    "dev": "rollup -wc rollup.conf.js",
    "prebuild": "npm run ci && npm run clean",
    "build": "npm run build:js",
