    "clean": "del dist",
    "build:js": "rollup -c rollup.conf.js",
    "prepublishOnly": "npm run build"
  },
  "repository": "git@github.com:gera2ld/json-formatter.git",
  "devDependencies": {
    "@gera2ld/plaid": "^2.0.0",
    "@gera2ld/plaid-common-react": "^2.0.0",
    "@gera2ld/plaid-rollup": "^2.0.0",
    "del-cli": "^3.0.0",
    "husky": "^4.2.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "@gera2ld/jsx-dom": "^1.1.2"
  }
}
